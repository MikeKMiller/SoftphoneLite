{
    "manifest_version": 2,
    "name": "Softphone",
    "version": "1.0",
    "description": "Softphone",
    "browser_action":
    {
        "default_popup": "popup.html"
    },
    "permissions": [
        "cookies",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage",
        "notifications"
    ],
    "icons":
    {
        "16": "ico/icon16.png",
        "48": "ico/icon48.png",
        "128": "ico/icon128.png"
    },
    "background":
    {
        "scripts": ["js/sip-0.13.7.min.js", "js/background.js"],
        "persistent": true
    },
    "options_ui":
    {
        "page": "options.html",
        "open_in_tab": false
    }
}